     <template id="main_page" name="Contacts Details">
        <t t-call="website.layout">
            <div class="oe_structure">
                <div class="container">
                    <br/>
                    <center>
                        <h3>Contacts Details</h3>
                    </center>
                    <br/>
                    <table class="table-striped table">
                        <thead style="font-size: 23px;">
                            <tr>
                                <h6>
                                    <b>
                                        <th>Name</th>
                                    </b>
                                </h6>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="key" t-as="rec">
                                <tr>
                                    <td>

                                            <span t-esc="rec.display_name"/>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
            </div>
        </t>
    </template>
